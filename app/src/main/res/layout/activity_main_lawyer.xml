<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.LawyerMainActivity">

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/menubar_main_lawyer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/lawyer_menubar"
        android:background="@color/primary"
        app:itemIconTint="@color/yellow"
        app:itemTextColor="@color/yellow"/>
    <FrameLayout
        android:id="@+id/frameLayout_lawyer_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/menubar_main_lawyer"/>


</androidx.constraintlayout.widget.ConstraintLayout>